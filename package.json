{
  "name": "git-temp",
  "version": "0.0.1",
  "description": "a tool for pushing your entire working directory to a temp branch automatically",
  "main": "lib/index.js",
  "bin": {
    "git-temp-server": "bin/git-temp-server.js"
  },
  "scripts": {
    "test": "no test",
    "pre-publish": "no build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ysmood/git-temp.git"
  },
  "keywords": [
    "git",
    "push",
    "tool"
  ],
  "author": "http://ysmood.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysmood/git-temp/issues"
  },
  "homepage": "https://github.com/ysmood/git-temp#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0"
  },
  "dependencies": {
    "babel-runtime": "^5.8.25",
    "nodegit": "^0.5.0",
    "nokit": "^0.14.7"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "double"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-console": [
        0
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  }
}
